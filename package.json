    "faker": "4.1.0",
    "husky": "0.14.3",
    "jest": "25.1.0",
    "jest-sonar-reporter": "2.0.0",
    "nodemon": "1.18.7",
    "sinon": "7.2.3",
    "sonarjs": "1.0.0",
    "supertest": "3.1.0",
    "ts-jest": "25.2.1",
    "tsc-watch": "2.2.1",
    "tslint": "5.10.0",
    "typedoc": "0.17.1",
    "typescript": "4.6.2"
