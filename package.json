  "main": "index.js",
  "private": false,
  "scripts": {
    "precommit": "CI=true yarn lint && yarn test",
    "prebuild": "npm run remove-dist",
    "build": "tsc",
    "postbuild": "npm run copy-resources",
    "prestart": "npm run remove-dist",
    "start": "cross-env NODE_ENV=local tsc-watch --project . --onSuccess \"npm run nodemon\"",
    "startup": "node ./dist/index.js",
    "prenodemon": "npm run copy-resources",
    "nodemon": "nodemon --inspect ./dist/index.js",
    "remove-dist": "rm -rf ./dist",
    "copy-resources": "",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test jest --forceExit --runInBand",
    "test:c": "npm run test -- --coverage",
    "test:w": "npm run test -- --watch",
    "commit": "git-cz",
    "push": "git push",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "yarn lint -- --fix"
  },
  "dependencies": {
    "body-parser": "1.18.3",
